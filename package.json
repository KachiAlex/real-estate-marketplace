{
  "name": "real-estate-marketplace",
  "version": "1.0.1",
  "description": "A comprehensive real estate marketplace platform with property sales, rentals, and investment features",
  "private": true,
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "build": "react-scripts build",
    "frontend:start": "react-scripts start",
    "frontend:build": "react-scripts build",
    "frontend:test": "react-scripts test",
    "frontend:test:ci": "set NODE_OPTIONS=--max-old-space-size=4096 && react-scripts test",
    "frontend:eject": "react-scripts eject",
    "frontend:serve": "serve -s build",
    "dev:full": "concurrently \"npm run dev\" \"npm run frontend:start\"",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:headless": "cypress run --headless",
    "test:all": "npm run test:e2e:headless",
    "test:comprehensive": "npm run test:e2e:headless"
  },
