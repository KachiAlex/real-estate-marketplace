    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.8",
    "chart.js": "^4.5.0",
    "cloudinary": "^1.40.0",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
