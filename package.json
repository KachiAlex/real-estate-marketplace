{
  "name": "real-estate-marketplace",
  "version": "1.0.1",
  "description": "A comprehensive real estate marketplace platform with property sales, rentals, and investment features",
  "private": true,
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "build": "react-scripts build",
    "frontend:start": "react-scripts start",
    "frontend:build": "react-scripts build",
    "frontend:test": "react-scripts test",
